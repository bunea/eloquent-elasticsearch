{
    "name": "mirceasoaica/eloquent-elasticsearch",
    "description": "",
    "keywords": [
        "laravel",
        "eloquent",
        "elasticsearch"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mircea Soaica",
            "email": "info@mirceasoaica.com"
        }
    ],
    "require": {
        "php": ">7.1.3",
        "illuminate/contracts": "5.6.*",
        "illuminate/database": "5.6.*",
        "illuminate/support": "5.6.*",
        "nesbot/carbon": "^1.26.3",
        "ruflin/elastica": "^6.0",
        "spatie/laravel-tags": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "nunomaduro/larastan": "^0.3.8",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "EloquentElastic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EloquentElastic\\Tests": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1"
        },
        "laravel": {
            "providers": [
                "EloquentElastic\\ServiceProvider"
            ]
        }
    }
}
